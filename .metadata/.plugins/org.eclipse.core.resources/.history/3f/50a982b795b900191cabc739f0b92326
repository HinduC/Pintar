package prog;

public class pat4 {

	public static void main(String[] args)
	{
		int num=4;
		for(int i=0;i<num;i++)
		{int n1=n1+1;
			for(int j=0;j<num;j++)
			{
				if(i>=j)
				{
					System.out.print(n1);
				}
				System.out.print(" ");
			}
			System.out.println(" ");
		}

	}

}
