package prog;

public class pat5 {

	public static void main(String[] args) 
	{
		int num=5;
		int n=0;
		for(int i=1;i<num;i++)
		{
			for(int j=1 ; j<num ; j++)
			{
				if(i+j>num-1)
				{
					System.out.print(i+n);
					n=n+i;
				}
				System.out.print(" ");
			}
			n=n+i;
			System.out.println(" ");
		}
	}
}
