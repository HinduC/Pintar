package prog;

public class prime {

	public static void main(String[] args) 
	{
		int temp=0,i,j;
		for(i=1 ; i<100 ; i++)
		{
			for(j=2 ; j<i ; j++)
			{
				if(i%j==0)
				{
					
					temp = temp+1;
				}
				
	
			}
			
		}
		if(temp ==0)
		{
			System.out.println(i+" is a prime number");
		}
		
	

	}

}
