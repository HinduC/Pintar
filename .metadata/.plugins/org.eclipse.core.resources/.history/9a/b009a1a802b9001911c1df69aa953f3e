package prog;

public class p1 
{
	public static void main(String[] args)
	{
		String str ="i love java";
		String[] word = str.split(str);
		for(int i=word.length-1 ; i>=0 ; i--)
		{
			if(i!=1)
			{
	
			System.out.println(word[i]+" ");
		}
		else
		{
			StringBuilder s = new StringBuilder(word[i].re);
			System.out.println(s);
		}
		}
	}

}
