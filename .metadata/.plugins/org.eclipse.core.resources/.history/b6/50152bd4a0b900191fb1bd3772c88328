package prog;

public class rev 
{
	public static void main(String[] args) 
	{
		String str="i love selenium java";
		String[] s=str.split(" ");
		String temp=" ";
		for (int i=0; i<s.length;i++)
		{
			for(int j=i; j<s.length-1;j++)
			{
				if(s[i].compareTo(s[j+1])>0)
				{
					temp=s[j];
					s[j]=s[j+1];
					s[j+1]=temp;
				}
			}
		}
		for(int i=0;i<s.length;i++)
		{
		System.out.println(s[i]);
		}
	}

}

