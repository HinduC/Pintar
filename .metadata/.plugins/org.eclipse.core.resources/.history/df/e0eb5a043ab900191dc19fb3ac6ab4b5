package prog;

public class prime {

	public static void main(String[] args) 
	{
		for(int i=1 ; i<100 ; i++)
		{
			for(int j=2 ; j<i ; j++)
			{
				if(i%j==0)
				{
					int temp=0;
					temp = temp+1;
				}
	
			}
			System.out.println(i+" is a prime number");
		}
	

	}

}
